{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "traceResolution": false,
    "declaration": false,
    "lib": [ "dom", "dom.iterable", "esnext" ],
    "jsx": "react-jsx",
    "jsxImportSource": "@toad",
    "baseUrl": "src",
    "paths": {
        "@toad": ["toad"],
        "@toad/*": ["*"]
    },
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "noImplicitOverride": true,
    "strict": true,
    "strictNullChecks": true,
    "allowJs": false,
    "noImplicitAny": true,
    "sourceMap": true
  }
}
