{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "traceResolution": false,
    "declaration": false,
    "lib": [ "es6", "dom", "dom.iterable" ],
    "jsx": "react-jsx",
    "jsxImportSource": "@toad",
    "baseUrl": ".",
    "paths": {
        "@toad": ["src/toad"],
        "@toad/*": ["src/*"]
    },
    "experimentalDecorators": true,
    "noImplicitOverride": true,
    "strict": true,
    "strictNullChecks": true,
    "allowJs": false,
    "noImplicitAny": true,
    "sourceMap": true
  }
}
